# Example .env for Portfolio/13-ExpressAuthentication
# Replace with your real credentials before running the app

# MongoDB connection string (include username:password@host:port/db if required)
MONGO_URI=mongodb://127.0.0.1:27017/LOTR

# Session secret used by express-session
SESSION_SECRET=replace_with_a_strong_secret

# Optional: PORT
PORT=3000

# Optional Google OAuth placeholders
#GOOGLE_CLIENT_ID=your-google-client-id
#GOOGLE_CLIENT_SECRET=your-google-client-secret
#GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/secrets
